# Default API Key (used when no provider-specific key is set)
R2C_DEFAULT_API_KEY=your-api-key-here

# Provider-specific API keys
R2C_PROVIDER_DEEPSEEK_API_KEY=sk-xxx
R2C_PROVIDER_ZHIPU_API_KEY=xxx

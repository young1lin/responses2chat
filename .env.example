R2C_PROVIDER_LONGCAT_API_KEY=ak_xxx
R2C_PROVIDER_STEPFUN_API_KEY=xxx
R2C_PROVIDER_DEEPINFRA_API_KEY=xxx

# Server configuration
R2C_PORT=8080
R2C_LOG_LEVEL=info
